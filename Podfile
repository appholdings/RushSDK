  pod 'RxSwift'
  pod 'RxCocoa'
  
  pod 'SwiftyStoreKit'
  
  pod 'FacebookSDK'
  pod 'Amplitude-iOS'
  pod 'Branch'
  pod 'Firebase/Analytics'
  pod 'Firebase/Messaging'
end
